{
  "name": "cursor-ai-platform",
  "version": "1.0.0",
  "description": "ðŸŒŠ DeepBlue Cursor AI Platform - Comprehensive AI-powered code editor with real-time collaboration",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm run test",
    "test:backend": "cd backend && npm run test",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "setup": "npm run setup:frontend && npm run setup:backend && npm run setup:database",
    "setup:frontend": "cd frontend && npm install",
    "setup:backend": "cd backend && npm install",
    "setup:database": "docker-compose up -d postgres redis"
  },
  "keywords": [
    "ai",
    "code-editor",
    "collaboration",
    "typescript",
    "react",
    "nodejs",
    "deepblue"
  ],
  "author": "DeepBlue Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
